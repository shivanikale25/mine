package problems;

public class Pb1 {

	public static void main(String[] args) {
    //sum of natural number 1 to 10
		int num=50; int num1=0;
		for(int i=1;i<=num;i++)
		{
			num1=num1+i;
		}
		System.out.println("sum is "+ num1);
	
	{
    // find factorial of number 5
	int fact=1;
	
	for(int i=1;i<=5;i++)
	{
		fact=fact*i;
	}
	System.out.println("fact is "+fact);
}
}
	//multiplication table 
	for
}