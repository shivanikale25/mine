package logicalprogramm;

public class PrimeNo {

	public static void main(String[] args) {
//1,3,5,7,11,13,17,19
		
		
		int a=7;
		int count =0;
		for(int i=2;i<a;i++)
		{
			if(a%i==0)
			{
				count++;
				break;
			}
			if(count==1)
			{
				System.out.println("given no is"+a+"is not prime");
			}
			else
			{
				System.out.println("given no is"+a+"is  prime");

			}
		}
	}

}
