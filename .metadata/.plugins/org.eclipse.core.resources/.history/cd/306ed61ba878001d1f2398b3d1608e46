package patternPrograms;

public class Ex03 {

	public static void main(String[] args) {

		
	/******
	 *****
	 ****
	 ***
	 **
	 */
		
		//int star=6;
		for(int i=1;i<=6;i++)
		{
			for(int j=1;j>=i;j--)
			{
				System.out.print("*");
				
			}
			//star--;
			
			System.out.println();
		}
		
	}

}
