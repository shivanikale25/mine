package seleniumsdy;

public class NestedIf {

	public static void main(String[] args) 
	{
    
		String username="shivani";
		String passw="shiv123";
		
		if(username=="shivani")
		{
			System.out.println("username is correct go for passwd");
			if(passw=="shiv123")
			{
				System.out.println("password is correct go to profile");
			}
			else {
				System.out.println("enter correct passwd");
			}
		}
		else
		{
			System.out.println("enter correct username");
			
		}
		
	}

}
